ctrl + shift - j : yabai -m window --focus west
ctrl + shift - k : yabai -m window --focus south
ctrl + shift - i : yabai -m window --focus north
ctrl + shift - l : yabai -m window --focus east

ctrl + shift - x : yabai -m window --close; yabai -m window --focus recent

# send window to space and then focus that space
ctrl + alt - 1 : yabai -m window --space  4; yabai -m space --focus 1
ctrl + alt - 2 : yabai -m window --space  4; yabai -m space --focus 2
ctrl + alt - 3 : yabai -m window --space  4; yabai -m space --focus 3
ctrl + alt - 4 : yabai -m window --space  4; yabai -m space --focus 4

ctrl + shift - 1 : yabai -m space --focus 1
ctrl + shift - 2 : yabai -m space --focus 2
ctrl + shift - 3 : yabai -m space --focus 3
ctrl + shift - 4 : yabai -m space --focus 4
